{
  "name": "freespaced",
  "version": "0.0.0",
  "scripts": {
    "start": "node dist/server",
    "start:dev": "cross-env NODE_ENV=development node dist/server",
    "watch": "tsc -w",
    "dev": "cross-env NODE_ENV=development nodemon dist/server",
    "dev2": "cross-env NODE_ENV=development nodemon --watch src  --exec 'ts-node' src/server.ts",
    "build": "tsc",
    "test-server":"cross-env NODE_ENV=test ts-node src/server.ts",
    "test": "cross-env NODE_ENV=test cross-env JWT_SECRET=jwt_secret jest  --detectOpenHandles",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.2.6",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.1.1",
    "hpp": "^0.2.3",
    "ioredis": "^4.17.3",
    "jdenticon": "^3.0.1",
    "jest": "^26.5.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.13",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^4.2.0",
    "swagger-ui-express": "^4.1.4",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typeorm": "^0.2.28",
    "typescript": "^4.0.3",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/helmet": "0.0.48",
    "@types/hpp": "^0.2.1",
    "@types/ioredis": "^4.17.5",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.5",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "nodemon": "^2.0.4",
    "supertest": "^5.0.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2"
  }
}
